<article>

<h1 class="page_title round">Events</h1>

<%= paginate @events %>

<table>
  <thead>
    <tr>
      <th>Event</th>
      <th>Description</th>
      <th>Date</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.title %></td>
        <td><%= event.description %></td>
        <td><%= event.date %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

</article>