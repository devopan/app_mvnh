<!DOCTYPE html>
<html>
<head>
  <title><%= title%></title>
  <%= csrf_meta_tags %>
  <%= javascript_include_tag "default", "data-turbolinks-track" => true %>
  <%= render 'layouts/stylesheets' %>
</head>
<body>

<article class="container">
<%= render 'layouts/header' %>
	<section class="round">
		
	<% if !(@title == "Home") %>
	<%= render_breadcrumbs :separator => ' > ' %>
	<% end %>



	<%= yield %>



	</section>
<%= render 'layouts/footer' %>
</article>
</body>
</html>
